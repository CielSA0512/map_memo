<h1>新規お気に入りスポット投稿</h1>

<%= form_with model: @post, local: true do |f| %>
  <div class="form-group">
    <%= f.label :address, 'スポット名' %><br>
    <%= f.text_field :address, placeholder: "東京スカイツリー", autofocus: true, class: "form-control", autocomplete: "off" %><br>
    <%= f.label :text, "メモ" %><br>
    <%= f.text_area :text, placeholder: "メモ" %>
  </div>
    <%= f.submit '投稿する' %> 
<% end %>